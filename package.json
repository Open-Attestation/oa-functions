{
  "name": "oa-functions",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "create-domain:verify": "cd ./src/verify && sls create_domain",
    "dev": "npm-run-all --parallel dev:*",
    "dev:email": "cd ./src/email && sls offline start",
    "dev:storage": "cd ./src/storage && sls offline start",
    "dev:verify": "cd ./src/verify && sls offline start",
    "deploy": "npm-run-all --sequential \"deploy:* -- {@}\" --",
    "deploy:email": "cd ./src/email && sls deploy",
    "deploy:storage": "cd ./src/storage && sls deploy",
    "deploy:verify": "cd ./src/verify && sls deploy",
    "s3-local-setup": "cd ./src/storage && sls plugin install --name serverless-s3-local",
    "sls-config-check": "npm-run-all --sequential sls-config-check::*",
    "sls-config-check:email": "cd ./src/email && sls print",
    "sls-config-check:storage": "cd ./src/storage && sls print",
    "sls-config-check:verify": "cd ./src/verify && sls print",
    "test": "jest --testPathPattern=src --testPathIgnorePatterns=src/e2e",
    "test:watch": "npm run test -- --watch",
    "test:e2e": "jest --testPathPattern=src/e2e",
    "test:e2e:local": "IS_OFFLINE=true npm run test:e2e --",
    "lint": "eslint . --ext .ts,.json --max-warnings 0",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "Raymond Yeh <yehjxraymond@gmail.com> (http://geek.sg)",
  "license": "ISC",
  "dependencies": {
    "@govtechsg/oa-encryption": "^1.3.3",
    "@govtechsg/oa-verify": "^7.4.3",
    "@govtechsg/open-attestation": "^5.3.0",
    "@middy/core": "^2.2.0",
    "@middy/http-cors": "^2.2.0",
    "@middy/http-error-handler": "^2.2.0",
    "@middy/http-json-body-parser": "^2.2.0",
    "@middy/validator": "^2.2.0",
    "@types/aws-lambda": "^8.10.76",
    "aws-sdk": "^2.913.0",
    "debug": "^4.3.1",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.1",
    "serverless": "^2.43.1",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "copy-webpack-plugin": "^9.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "jest": "^27.0.1",
    "jest-raw-loader": "^1.0.1",
    "jest-watch-typeahead": "^0.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "raw-loader": "^4.0.2",
    "runtypes": "^6.3.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-offline": "^7.0.0",
    "serverless-s3-local": "^0.6.15",
    "serverless-s3-remover": "^0.6.0",
    "serverless-webpack": "^5.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-node-externals": "^3.0.0"
  }
